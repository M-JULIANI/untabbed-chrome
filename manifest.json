{
    "manifest_version": 3,
    "name": "Untabbed-Chrome",
    "version": "1.0.0",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "action": {
        "default_title": "Untabbed"
    },
    "background": {
        "service_worker": "src/service-worker.js"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
      }
}