{
    "manifest_version": 3,
    "name": "Untabbed-Chrome",
    "version": "1.0.0",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs",
        "https://cdn.jsdelivr.net/npm/@tensorflow/*",
        "https://cdn.jsdelivr.net/npm/@tensorflow-models/universal-sentence-encoder/*"
    ],
    "action": {
        "default_title": "Untabbed"
    },
    "background": {
        "service_worker": "src/service-worker.js"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self' http://localhost:5000; object-src 'self';"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}