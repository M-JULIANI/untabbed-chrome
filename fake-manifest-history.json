{
    "manifest_version": 3,
    "name": "Untabbed-Chrome",
    "version": "1.0.0",
    "content_scripts": [
        {
          "js": ["src/listener.js"],
          "matches": ["<all_urls>"]
        },
        {
            "js": ["src/website-embedder.js"],
            "matches": ["<all_urls>"]
        }
      ],
    "action": {
        "default_title": "Untabbed"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": [
        "sidePanel",
        "storage",
        "activeTab",
        "scripting",
        "tabs"
      ],
      "background": {
        "service_worker": "src/service-worker.js"
      }
}